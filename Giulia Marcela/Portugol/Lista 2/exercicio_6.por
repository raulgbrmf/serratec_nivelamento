programa
{
	
	funcao inicio()
	{
		// Variaveis 
		inteiro i = 1, num

		//Entrada de dados
		escreva ("Digite um número: ")
		leia (num)

		//Saida de dados
		enquanto(i<=num){
			escreva(i, " ")
			i = i + 1
		}
	}
}

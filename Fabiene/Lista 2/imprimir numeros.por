programa
{
	inteiro num
	inteiro contador=1
	
	funcao inicio()
	{
		escreva("Digite um número: ")
		leia (num)

		limpa()

		enquanto (contador<=num)
		{
			
			escreva(contador, " ")
			contador=contador+1
		}
	}
}

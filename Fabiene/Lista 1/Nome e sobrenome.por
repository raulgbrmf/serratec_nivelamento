programa
{
	cadeia nome
	cadeia sobrenome
	funcao inicio()
	{
		escreva ("Digite seu nome: ")
		leia (nome)
		escreva ("Digite seu sobrenome: ")
		leia (sobrenome)
		escreva ("Meu nome é: ",nome, " ", sobrenome)
	}
}
